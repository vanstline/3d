{
  "private": true,
  "name": "imp-3dinteractive",
  "version": "0.0.1",
  "description": "umi typescript template",
  "author": "",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run dev:local",
    "analyz": "ANALYZE=1 umi dev",
    "dev:local": "cross-env SOCKET_SERVER=http://localhost:7070 UMI_ENV=local umi dev --env=local",
    "dev:autopush": "cross-env SOCKET_SERVER=http://localhost:7070 UMI_ENV=prod umi dev --env=autopush",
    "dev:staging": "cross-env SOCKET_SERVER=http://localhost:7070 UMI_ENV=prod umi dev --env=staging",
    "dev:preprod": "cross-env SOCKET_SERVER=http://localhost:7070 UMI_ENV=prod umi dev --env=preprod",
    "dev:prod": "cross-env SOCKET_SERVER=http://localhost:7070 UMI_ENV=prod umi dev --env=prod",
    "build:local": "cross-env UMI_ENV=local umi build --env=local",
    "build:autopush": "cross-env UMI_ENV=prod umi build --env=autopush",
    "build:staging": "cross-env UMI_ENV=prod umi build --env=staging",
    "build:preprod": "cross-env UMI_ENV=prod umi build --env=preprod",
    "build:prod": "cross-env UMI_ENV=prod umi build --env=prod",
    "build:proto": "npm run gen:proto",
    "clean": "npm run clean:cache && rimraf ./node_modules/ ./yarn.lock ./package-lock.json ./.git/hooks/pre-commit",
    "clean:cache": "rimraf ./dist/ ./build/ ./src/.umi/ ./src/.umi-production/",
    "eslint": "eslint --ext .jsx,.js,.ts,.tsx \"src/**\" --config .eslintrc.prod.js --ignore-path .eslintignore",
    "stylelint": "stylelint \"./src/**/*.{css,less}\" --config .stylelintrc.js --ignore-path .stylelintignore --allow-empty-input",
    "postinstall": "umi generate tmp",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "gen:proto": "node ./config/grpc.build.js",
    "eptools": "eptools deploy -f publish.yaml -v"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{css,less}": [
      "npm run stylelint"
    ],
    "*.{js,ts}?(x)": [
      "npm run eslint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "6.5.0",
    "antd": "4.20.0",
    "antd-dayjs-webpack-plugin": "1.0.6",
    "clsx": "^1.1.1",
    "dat.gui": "^0.7.9",
    "dayjs": "1.11.2",
    "ecoplants-lib": "1.1.7",
    "google-protobuf": "^3.20.1",
    "grpc-web": "^1.3.1",
    "mobx": "6.3.7",
    "mobx-react": "7.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "three": "^0.141.0",
    "three.ar.js": "^0.1.8",
    "umi": "3.5.20"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-react": "7.16.7",
    "@types/dat.gui": "^0.7.7",
    "@types/express": "4.17.13",
    "@types/history": "4.7.9",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/three": "^0.140.0",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@umijs/fabric": "2.8.1",
    "@umijs/plugin-qiankun": "2.36.0",
    "@umijs/preset-react": "2.1.0",
    "@umijs/test": "3.5.20",
    "babel-plugin-transform-modules": "0.1.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-airbnb-typescript": "16.0.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "file-loader": "^6.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "postcss": "8.4.12",
    "postcss-less": "6.0.0",
    "prettier": "2.2.0",
    "protoc-build": "1.0.4",
    "rimraf": "2.7.1",
    "stylelint": "14.6.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-order": "5.0.0",
    "typescript": "4.5.4",
    "yorkie": "2.0.0"
  }
}
